{
  "name": "developer-tools-pwa",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "A modern Progressive Web Application providing essential developer utilities",
  "keywords": ["pwa", "developer-tools", "json-formatter", "text-compare", "sql-formatter"],
  "scripts": {
    "dev": "vite",
    "dev:json-formatter": "VITE_BUILD_APP_ID=json-formatter vite",
    "dev:text-compare": "VITE_BUILD_APP_ID=text-compare vite",
    "dev:indexeddb-crud": "VITE_BUILD_APP_ID=indexeddb-crud vite",
    "dev:jwt-decoder": "VITE_BUILD_APP_ID=jwt-decoder vite",
    "dev:expense-manager": "VITE_BUILD_APP_ID=expense-manager vite",
    "build": "vite build",
    "build:json-formatter": "VITE_BUILD_APP_ID=json-formatter vite build",
    "build:text-compare": "VITE_BUILD_APP_ID=text-compare vite build",
    "build:indexeddb-crud": "VITE_BUILD_APP_ID=indexeddb-crud vite build",
    "build:jwt-decoder": "VITE_BUILD_APP_ID=jwt-decoder vite build",
    "build:expense-manager": "VITE_BUILD_APP_ID=expense-manager vite build",
    "preview:json-formatter": "vite preview",
    "preview:text-compare": "vite preview",
    "preview:indexeddb-crud": "vite preview",
    "preview:jwt-decoder": "vite preview",
    "preview:expense-manager": "vite preview",
    "lint": "biome check .",
    "lint:fix": "biome check . --apply",
    "format": "biome format . --write",
    "type-check": "tsc --noEmit",
    "verify": "pnpm lint && pnpm type-check && pnpm build",
    "preview": "vite preview",
    "prepare": "husky",
    "deploy:prod:expense-manager": "vercel --prod --local-config vercel.json",
    "deploy:preview:expense-manager": "vercel --local-config vercel.json"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/cli": "next",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "dexie": "^4.2.0",
    "lucide-react": "^0.544.0",
    "next-themes": "^0.4.6",
    "react": "19",
    "react-day-picker": "^9.9.0",
    "react-dom": "19",
    "react-hook-form": "^7.62.0",
    "react-resizable-panels": "^3.0.6",
    "react-router-dom": "^6.26.2",
    "recharts": "2.15.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "4",
    "workbox-precaching": "^7.1.0",
    "workbox-routing": "^7.1.0",
    "workbox-window": "^7.1.0",
    "zod": "^4.1.8",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.3",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^24.3.3",
    "@types/react": "^18.3.8",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.21",
    "husky": "^9.1.6",
